generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Exercise {
  id          Int      @id @default(autoincrement())
  title       String   @unique
  description String
  images      String
  video       String?
  createdAt   DateTime @default(now())
}
